<script language="javascript">
frm = document.addr;
MyContactsList = frm.MyContacts;
MyDestinationList = frm.MyDestination;
function IsIDAdded(ID)
{
var ret=false-+;
for (var i=0;i<MyDestinationList.length;i++)
{
if (ID==MyDestinationList.options[i].value)
ret=true;
}
return ret;
}
function DoMSL()
{
var TheTable = e = window.event.srcElement;
while (TheTable.tagName!="TABLE")
{TheTable=TheTable.parentNode;}
while (e.tagName!="TR")
{e=e.parentNode;}
if (window.event.shiftKey)
{	
var lastTr = TheTable.rows[0];
for (var i=0;i<TheTable.rows.length;i++)
if (TheTable.rows[i].getAttribute("lastSelected") == "true")
lastTr = TheTable.rows[i];
if (lastTr == null)
return;
for (var i=0;i<TheTable.rows.length;i++)
{
if (between(i, e.rowIndex, lastTr.rowIndex) == true)
TheTable.rows[i].className='SL';
else
TheTable.rows[i].className='';
}
}
else if (window.event.ctrlKey)
{	
e.className = e.className == 'SL' ? "": "SL";
}
else
{
for (var i=0;i<TheTable.rows.length;i++)
TheTable.rows[i].className='';
e.className='SL';		
}
if (!window.event.shiftKey)
{
for (var i=0;i<TheTable.rows.length;i++)
TheTable.rows[i].lastSelected = "false";
e.setAttribute("lastSelected", "true");
}
}
function between(n, a, b)
{
var min, max;
if (a<b){
min = a;
max = b;
}
else{
min = b;
max = a;
}
if ((n > min && n < max) || (n == min) || (n == max))
return true;
else
return false;
}
function AddToTheList() 
{
if ( (MyContactsList.selectedIndex < 0) &&(frm.emailAddr.value.length==0) )
{
Err("150995598");
if (MyContactsList.options[0] != null)
MyContactsList.options[0].selected = true;
MyContactsList.focus();
return false;
}
else
{
if (frm.emailAddr.value.length>0)
{
if (ValE(frm.emailAddr))
return false;
else
{
if (!IsIDAdded(frm.emailAddr.value))
{
MyDestinationList.options[MyDestinationList.length] = new Option(frm.emailAddr.value,frm.emailAddr.value);
frm.emailAddr.value="";
}
}
}
else
{
if (!IsIDAdded(MyContactsList.options[MyContactsList.selectedIndex].value))
{
MyDestinationList.options[MyDestinationList.length] = new Option(MyContactsList.options[MyContactsList.selectedIndex].text,MyContactsList.options[MyContactsList.selectedIndex].value);
}
}
}
}
function RemoveFromTheList() 
{
if ( MyDestinationList.selectedIndex < 0)
{
Err("150995598");
if (MyDestinationList.options[0] != null)
MyDestinationList.options[0].selected = true;
MyDestinationList.focus();
return false;
}
else
{
MyDestinationList.options[MyDestinationList.selectedIndex] = null;
}
}
function HandleSubmit(S)
{
var SelectedList = "";
for(var i = 0; i < MyDestinationList.length; i++) 
{
SelectedList += (MyDestinationList.options[i].value + ",");		
}
SelectedList = SelectedList.substring(0, SelectedList.length-1);
frm.SelectedContactsList.value = SelectedList;
if (S!=1)
{
frm._HMaction.value="Submit";
frm.submit();
}
}
</script>
</td>
</tr>
</table>
<script language="javascript">
frm = document.addr;
IsABmigrationComplete = "1";
ContactType = "";
isMessengerMigrated = "1";
IsManagedRestrictedUser = "0";
function EC(TheTR,img)
{
var DataTR = eval('document.getElementById("' + TheTR + '")');
if (DataTR.style.visibility=="visible" || DataTR.style.visibility=="" )
{
DataTR.style.visibility="hidden";
DataTR.style.position="absolute";
DataTR.parentNode.style.padding="0 0 0 0";
img.childNodes[1].childNodes[0].src='http://207.68.162.24/i.p.arr.down.gif';
}
else
{
DataTR.style.visibility="visible";
DataTR.style.position="relative";
DataTR.parentNode.style.padding="9 10 14 10";
img.childNodes[1].childNodes[0].src='http://207.68.162.24/i.p.arr.up.gif';
}
}
function DOIIM()
{
}
function ValidateRequired(AddAnother) {
var err="";
var emptyObj=null;
if ( frm.alias.value == "")
emptyObj = frm.alias;
if (emptyObj)
{
Err("150995038");
emptyObj.focus();
err=true;
}
else
{
frm.encodedalias.value = DE(frm.alias.value);
HandleSubmit(1);
if (err==false)
{
if (AddAnother)
frm.addanother.value='1';
frm.submit();
}
}
}
function ValE(objAddr)
{
var rt = false;
if (objAddr.value!="")
{
if (!ValidateEmail(objAddr.value))
{
Err("150995068");
objAddr.focus();
rt = true;
}
}
return rt;
}
